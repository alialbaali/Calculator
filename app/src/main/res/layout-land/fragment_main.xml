<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/resultView"
            style="@style/resultTextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginStart="@dimen/high_margin"
            android:layout_marginTop="@dimen/high_margin"
            android:layout_marginEnd="@dimen/high_margin"
            android:layout_marginBottom="@dimen/high_margin"
            android:layout_weight="2" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/high_margin"
            android:layout_marginEnd="@dimen/high_margin"
            android:layout_marginBottom="@dimen/high_margin"
            android:layout_weight="10">


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <Button
                    android:id="@+id/clearButton"
                    style="@style/landClearButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/clear" />

                <Button
                    android:id="@+id/parenthesisButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/parenthesis"
                    android:textSize="@dimen/fit_button_text_size"/>

                <Button
                    android:id="@+id/percentageButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/percentage"
                    android:textSize="@dimen/fit_button_text_size"
                     />

                <Button
                    android:id="@+id/divisionButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/division" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <Button
                    android:id="@+id/button7"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/seven" />

                <Button
                    android:id="@+id/button8"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/eight" />

                <Button
                    android:id="@+id/button9"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/nine" />

                <Button
                    android:id="@+id/multiplyButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/multiply"
                    />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <Button
                    android:id="@+id/button4"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/four" />

                <Button
                    android:id="@+id/button5"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/five" />

                <Button
                    android:id="@+id/button6"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/six" />

                <Button
                    android:id="@+id/minusButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/minus" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <Button
                    android:id="@+id/button1"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/one" />

                <Button
                    android:id="@+id/button2"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/two" />

                <Button
                    android:id="@+id/button3"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/three" />

                <Button
                    android:id="@+id/plusButton"
                    style="@style/landSignButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/plus" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <Button
                    android:id="@+id/plusMinusButton"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/test" />

                <Button
                    android:id="@+id/button0"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/zero" />

                <Button
                    android:id="@+id/dotButton"
                    style="@style/landButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/dot" />

                <Button
                    android:id="@+id/equalButton"
                    style="@style/landEqualButtonShape"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="@dimen/normal_margin"
                    android:layout_weight="1"
                    android:text="@string/equal" />
            </TableRow>

        </TableLayout>
    </LinearLayout>

    <data>

    </data>

</layout>